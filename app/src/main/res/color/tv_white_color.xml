<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" android:color="@color/main_press_color" />

    <item android:state_selected="true" android:color="@color/main_press_color" />

    <item android:color="@color/color_999999" android:state_enabled="false" />

    <!--这个没写的state的要放在最后，否则没有效果-->
    <item android:color="@color/white" android:state_pressed="false" />
    <item android:color="@color/white" android:state_selected="false" />
</selector>